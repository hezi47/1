
{
	  int a;
	  scanf_s("%d", &a);
	  int b, c, d;
	  int f = 0;
	  for (b = a; b <= a + 3; b++) {
		  for (c = a; c <= a + 3; c++) {
			  for (d = a; c <= a + 3; d++) {
				  if (b!=c && b!=d && c!=d) {
						  printf("%d%d%d", b, c, d);
						  f++;
						  if (f%6==0) {
							  printf("\n");
						  }else {
							  printf(" ");
						  }

				  }
			  }
		  }
	  }
	  return 0;
    }
